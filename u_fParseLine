					ElseIf substr(cBuffer,006,4) $ ("1010") .AND. SB1->B1_UM ="ML" //SACO   Por Luiz Otavio p/ trazer o valor em Milheiro
						nKgMl:= (SB1->B1_XLARG*100)*(SB1->B1_XCOMP*100)*SB1->B1_XALT*0.922 // convert para Milheiro
						_xnXCUSTO := Round(nKgMl*Val(substr(cBuffer,099,10))/100,2)
						_xnPRCVEN := Round(nKgMl*(Val(substr(cBuffer,109,10))/100),2)
